        "helmet": "^7.1.0",
        "html-to-pdfmake": "^2.5.1",
        "morgan": "^1.10.0",
        "ng2-charts": "^6.0.1",
        "ngx-bootstrap": "^6.2.0",
        "ngx-cookie-service": "^17.0.0",
        "ngx-drag-scroll": "^2.1.1",
        "ngx-scrollbar": "^13.0.3",
        "pdfmake": "^0.2.8",
