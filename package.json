        "autoprefixer": "^10.4.19",
        "jasmine-core": "~4.6.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "postcss": "^8.4.39",
        "tailwindcss": "^3.4.4",
        "typescript": "~5.2.2"
    }
