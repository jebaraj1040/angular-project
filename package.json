{
    "name": "shfl-customer-portal-fe",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration dev",
        "test": "ng test",
        "serve:ssr:SHFL-CUSTOMER-PORTAL-FE": "node dist/SHFL-CUSTOMER-PORTAL-FE/server/server.mjs",
        "build-ssr:dev": "ng build --configuration=dev",
        "build-ssr:sit": "ng build --configuration=sit",
        "build-ssr:uat": "ng build --configuration=uat",
        "build-ssr:live": "ng build --configuration=live",
        "serve-ssr:dev": "node dist/SHFL-CUSTOMER-PORTAL-FE/server/server.mjs",
        "serve-ssr:sit": "node dist-sit/SHFL-CUSTOMER-PORTAL-FE/server/server.mjs",
        "serve-ssr:uat": "node dist-uat/SHFL-CUSTOMER-PORTAL-FE/server/server.mjs",
        "serve-ssr:live": "node dist-live/SHFL-CUSTOMER-PORTAL-FE/server/server.mjs",
        "scss-env-dev": "node src/styles/environments/scss.variable.dev.js",
        "scss-env-sit": "node src/styles/environments/scss.variable.sit.js",
        "scss-env-uat": "node src/styles/environments/scss.variable.uat.js",
        "scss-env-live": "node src/styles/environments/scss.variable.prod.js",
        "build-serve-ssr:dev": "npm run scss-env-dev && npm run build-ssr:dev && npm run serve-ssr:dev & ng serve",
        "build-serve-ssr:sit": "npm run scss-env-sit && npm run build-ssr:sit && npm run serve-ssr:sit & ng serve",
        "build-serve-ssr:uat": "npm run scss-env-uat && npm run build-ssr:uat && npm run serve-ssr:uat & ng serve",
        "build-serve-ssr:live": "npm run scss-env-live && npm run build-ssr:live && npm run serve-ssr:live & ng serve"
    },
    "private": true,
    "dependencies": {
